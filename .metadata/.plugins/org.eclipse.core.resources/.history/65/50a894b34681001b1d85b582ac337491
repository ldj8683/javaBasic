package 형변환;

public class 참조형형변환1 {

	public static void main(String[] args) {
		사람 p = new 사람();
		슈퍼맨 s = new 슈퍼맨();
		맨 m = new 맨();
		우먼 w = new 우먼();
		
		// 참조형은 상속관계에 있을 대만 가능한다
		// 부모 자식간의 관계는 형변환이 가능해서 대입이 가능하다
		p = s; // p가 가장 큼, s는 가장 작다 (자동형변환) , 부모 자식간의 관계만(상속) 가능하다. 형재는 불가능하다
		p = w; // 큰 <--- 작, upCasting
		//m = w; // 자식간에 누가 큰지 알수가없다.
	}

}

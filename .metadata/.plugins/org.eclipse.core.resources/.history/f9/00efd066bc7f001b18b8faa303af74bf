package 상속;

public class company {

	public static void main(String[] args) {
		Employee e = new Employee();
		e.name = "홍길동";
		e.address = "강남구";
		e.salary = 300;
		e.rrn = 20000201;
		
		System.out.println(e);
		
		Manager m = new Manager();
		m.name = "홍길동";
		m.address = "강남구";
		m.salary = 500;
		m.rrn = 19980201;
		m.test();
		System.out.println(m);
	}

}

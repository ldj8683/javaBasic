package conditional;

import javax.swing.JFrame;
import java.awt.Color;

public class Graphic03 {

	public static void main(String[] args) {
		JFrame f = new JFrame();
		f.getContentPane().setBackground(Color.GREEN);
		f.getContentPane().setLayout(null);
		f.setSize(600, 600);
		f.setVisible(true);
	}

}

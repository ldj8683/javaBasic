package 생성자;

public class Day {
	// 전역변수 선언
	String doing;
	int time;
	String place;
	
	// Day 생성자 메서드 생성
	public Day(String doing, int time, String place) {
		this.doing = doing;
		this.time = time;
		this.place = place;
	}

	
	@Override // toString 재정의
	public String toString() {
		return "Day [하는 일=" + doing + ", 시간=" + time + ", 장소=" + place + "]";
	}
	
}
